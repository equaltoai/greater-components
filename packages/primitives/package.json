{
  "name": "@equaltoai/greater-components-primitives",
  "private": true,
  "version": "1.0.17",
  "type": "module",
  "description": "Primitive UI components for Greater Components",
  "license": "AGPL-3.0-only",
  "author": "Greater Contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/greater/greater-components.git",
    "directory": "packages/primitives"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./Button": {
      "types": "./dist/components/Button.svelte.d.ts",
      "svelte": "./dist/components/Button.svelte.js",
      "import": "./dist/components/Button.svelte.js"
    },
    "./Modal": {
      "types": "./dist/components/Modal.svelte.d.ts",
      "svelte": "./dist/components/Modal.svelte.js",
      "import": "./dist/components/Modal.svelte.js"
    },
    "./TextField": {
      "types": "./dist/components/TextField.svelte.d.ts",
      "svelte": "./dist/components/TextField.svelte.js",
      "import": "./dist/components/TextField.svelte.js"
    },
    "./TextArea": {
      "types": "./dist/components/TextArea.svelte.d.ts",
      "svelte": "./dist/components/TextArea.svelte.js",
      "import": "./dist/components/TextArea.svelte.js"
    },
    "./Select": {
      "types": "./dist/components/Select.svelte.d.ts",
      "svelte": "./dist/components/Select.svelte.js",
      "import": "./dist/components/Select.svelte.js"
    },
    "./Checkbox": {
      "types": "./dist/components/Checkbox.svelte.d.ts",
      "svelte": "./dist/components/Checkbox.svelte.js",
      "import": "./dist/components/Checkbox.svelte.js"
    },
    "./Switch": {
      "types": "./dist/components/Switch.svelte.d.ts",
      "svelte": "./dist/components/Switch.svelte.js",
      "import": "./dist/components/Switch.svelte.js"
    },
    "./FileUpload": {
      "types": "./dist/components/FileUpload.svelte.d.ts",
      "svelte": "./dist/components/FileUpload.svelte.js",
      "import": "./dist/components/FileUpload.svelte.js"
    },
    "./Menu": {
      "types": "./dist/components/Menu.svelte.d.ts",
      "svelte": "./dist/components/Menu.svelte.js",
      "import": "./dist/components/Menu.svelte.js"
    },
    "./Tooltip": {
      "types": "./dist/components/Tooltip.svelte.d.ts",
      "svelte": "./dist/components/Tooltip.svelte.js",
      "import": "./dist/components/Tooltip.svelte.js"
    },
    "./Tabs": {
      "types": "./dist/components/Tabs.svelte.d.ts",
      "svelte": "./dist/components/Tabs.svelte.js",
      "import": "./dist/components/Tabs.svelte.js"
    },
    "./Avatar": {
      "types": "./dist/components/Avatar.svelte.d.ts",
      "svelte": "./dist/components/Avatar.svelte.js",
      "import": "./dist/components/Avatar.svelte.js"
    },
    "./Skeleton": {
      "types": "./dist/components/Skeleton.svelte.d.ts",
      "svelte": "./dist/components/Skeleton.svelte.js",
      "import": "./dist/components/Skeleton.svelte.js"
    },
    "./ThemeSwitcher": {
      "types": "./dist/components/ThemeSwitcher.svelte.d.ts",
      "svelte": "./dist/components/ThemeSwitcher.svelte.js",
      "import": "./dist/components/ThemeSwitcher.svelte.js"
    },
    "./ThemeProvider": {
      "types": "./dist/components/ThemeProvider.svelte.d.ts",
      "svelte": "./dist/components/ThemeProvider.svelte.js",
      "import": "./dist/components/ThemeProvider.svelte.js"
    },
    "./components/*": {
      "types": "./dist/components/*.svelte.d.ts",
      "svelte": "./dist/components/*.svelte.js",
      "import": "./dist/components/*.svelte.js"
    },
    "./stores/*": {
      "types": "./dist/stores/*.d.ts",
      "import": "./dist/stores/*.js"
    },
    "./style.css": "./dist/style.css",
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "src",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "svelte-package --output dist && node scripts/build-css.js",
    "dev": "svelte-package --output dist --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@equaltoai/greater-components-tokens": "workspace:*",
    "svelte": "^5.43.6"
  },
  "devDependencies": {
    "@equaltoai/greater-components-tokens": "workspace:*",
    "@sveltejs/package": "^2.5.4",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@testing-library/svelte": "^5.2.9",
    "@types/node": "^24.10.1",
    "@vitest/coverage-v8": "^4.0.9",
    "jsdom": "^27.1.0",
    "svelte": "^5.43.6",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vitest": "^4.0.9"
  }
}
