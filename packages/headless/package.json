{
  "name": "@equaltoai/greater-components-headless",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "description": "Headless UI primitives for Greater Components - behavior without styling",
  "license": "AGPL-3.0-only",
  "author": "Greater Contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/equaltoai/greater-components.git",
    "directory": "packages/headless"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./button": {
      "types": "./dist/primitives/button.d.ts",
      "import": "./dist/primitives/button.js"
    },
    "./modal": {
      "types": "./dist/primitives/modal.d.ts",
      "import": "./dist/primitives/modal.js"
    },
    "./menu": {
      "types": "./dist/primitives/menu.d.ts",
      "import": "./dist/primitives/menu.js"
    },
    "./tooltip": {
      "types": "./dist/primitives/tooltip.d.ts",
      "import": "./dist/primitives/tooltip.js"
    },
    "./tabs": {
      "types": "./dist/primitives/tabs.d.ts",
      "import": "./dist/primitives/tabs.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "svelte": "^5.43.6"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@testing-library/svelte": "^5.2.9",
    "@types/node": "^24.10.1",
    "@vitest/coverage-v8": "^4.0.14",
    "jsdom": "^27.2.0",
    "svelte": "^5.43.14",
    "typescript": "^5.9.3",
    "vite": "^7.2.4",
    "vitest": "^4.0.14"
  },
  "dependencies": {
    "focus-trap": "^7.6.6",
    "tabbable": "^6.3.0"
  }
}
