{
  "name": "@equaltoai/greater-components",
  "version": "4.2.6",
  "type": "module",
  "description": "Aggregate Greater Components library for EqualToAI",
  "license": "AGPL-3.0-only",
  "author": "Greater Contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/equaltoai/greater-components.git",
    "directory": "packages/greater-components"
  },
  "homepage": "https://github.com/equaltoai/greater-components#readme",
  "bugs": {
    "url": "https://github.com/equaltoai/greater-components/issues"
  },
  "keywords": [
    "svelte",
    "components",
    "ui",
    "fediverse",
    "social",
    "equaltoai"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": [
    "**/*.css"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./primitives": {
      "types": "./dist/primitives/index.d.ts",
      "svelte": "./dist/primitives/index.js",
      "import": "./dist/primitives/index.js",
      "default": "./dist/primitives/index.js"
    },
    "./primitives/*": {
      "types": "./dist/primitives/components/*.svelte.d.ts",
      "svelte": "./dist/primitives/components/*.svelte.js",
      "import": "./dist/primitives/components/*.svelte.js"
    },
    "./primitives/theme.css": "./dist/primitives/theme.css",
    "./primitives/styles.css": "./dist/primitives/styles.css",
    "./primitives/style.css": "./dist/primitives/style.css",
    "./tokens": {
      "types": "./dist/tokens/index.d.ts",
      "import": "./dist/tokens/index.js"
    },
    "./tokens/theme.css": "./dist/tokens/theme.css",
    "./tokens/high-contrast.css": "./dist/tokens/high-contrast.css",
    "./tokens/*": "./dist/tokens/*",
    "./icons": {
      "types": "./dist/icons/index.d.ts",
      "svelte": "./dist/icons/index.js",
      "import": "./dist/icons/index.js",
      "default": "./dist/icons/index.js"
    },
    "./icons/*": {
      "types": "./dist/icons/*.svelte.d.ts",
      "svelte": "./dist/icons/*.svelte.js",
      "import": "./dist/icons/*.svelte.js"
    },
    "./headless": {
      "types": "./dist/headless/index.d.ts",
      "import": "./dist/headless/index.js"
    },
    "./headless/*": {
      "types": "./dist/headless/*.d.ts",
      "import": "./dist/headless/*.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js"
    },
    "./utils/*": "./dist/utils/*",
    "./content": {
      "types": "./dist/content/index.d.ts",
      "svelte": "./dist/content/index.js",
      "import": "./dist/content/index.js"
    },
    "./shared/auth": {
      "types": "./dist/shared/auth/index.d.ts",
      "svelte": "./dist/shared/auth/src/index.ts",
      "import": "./dist/shared/auth/index.js"
    },
    "./shared/admin": {
      "types": "./dist/shared/admin/index.d.ts",
      "svelte": "./dist/shared/admin/src/index.ts",
      "import": "./dist/shared/admin/index.js"
    },
    "./shared/compose": {
      "types": "./dist/shared/compose/index.d.ts",
      "svelte": "./dist/shared/compose/src/index.ts",
      "import": "./dist/shared/compose/index.js"
    },
    "./shared/messaging": {
      "types": "./dist/shared/messaging/index.d.ts",
      "svelte": "./dist/shared/messaging/src/index.ts",
      "import": "./dist/shared/messaging/index.js"
    },
    "./shared/search": {
      "types": "./dist/shared/search/index.d.ts",
      "svelte": "./dist/shared/search/src/index.ts",
      "import": "./dist/shared/search/index.js"
    },
    "./shared/notifications": {
      "types": "./dist/shared/notifications/index.d.ts",
      "svelte": "./dist/shared/notifications/src/index.ts",
      "import": "./dist/shared/notifications/index.js"
    },
    "./shared/chat": {
      "types": "./dist/shared/chat/index.d.ts",
      "svelte": "./dist/shared/chat/src/index.ts",
      "import": "./dist/shared/chat/index.js"
    },
    "./chat": {
      "types": "./dist/shared/chat/index.d.ts",
      "svelte": "./dist/shared/chat/src/index.ts",
      "import": "./dist/shared/chat/index.js"
    },
    "./faces/social": {
      "types": "./dist/faces/social/index.d.ts",
      "svelte": "./dist/faces/social/src/index.ts",
      "import": "./dist/faces/social/index.js"
    },
    "./faces/social/*": {
      "types": "./dist/faces/social/*.d.ts",
      "svelte": "./dist/faces/social/src/*.svelte",
      "import": "./dist/faces/social/*.js"
    },
    "./faces/social/style.css": "./dist/faces/social/theme.css",
    "./adapters": {
      "types": "./dist/adapters/index.d.ts",
      "import": "./dist/adapters/index.js"
    },
    "./adapters/*": {
      "types": "./dist/adapters/*.d.ts",
      "import": "./dist/adapters/*.js"
    },
    "./testing": {
      "types": "./dist/testing/index.d.ts",
      "import": "./dist/testing/index.js"
    },
    "./testing/*": "./dist/testing/*",
    "./cli": {
      "types": "./dist/cli/index.d.ts",
      "import": "./dist/cli/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "node scripts/build.js",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "peerDependencies": {
    "isomorphic-dompurify": "^2.0.0",
    "svelte": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "isomorphic-dompurify": {
      "optional": true
    }
  },
  "dependencies": {
    "@apollo/client": "^4.0.9",
    "@apollo/client-react-streaming": "^0.14.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "graphql": "^16.12.0",
    "graphql-ws": "^6.0.6"
  },
  "devDependencies": {
    "@equaltoai/greater-components-adapters": "workspace:*",
    "@equaltoai/greater-components-admin": "workspace:*",
    "@equaltoai/greater-components-auth": "workspace:*",
    "@equaltoai/greater-components-chat": "workspace:*",
    "@equaltoai/greater-components-cli": "workspace:*",
    "@equaltoai/greater-components-compose": "workspace:*",
    "@equaltoai/greater-components-content": "workspace:*",
    "@equaltoai/greater-components-headless": "workspace:*",
    "@equaltoai/greater-components-icons": "workspace:*",
    "@equaltoai/greater-components-messaging": "workspace:*",
    "@equaltoai/greater-components-notifications": "workspace:*",
    "@equaltoai/greater-components-primitives": "workspace:*",
    "@equaltoai/greater-components-search": "workspace:*",
    "@equaltoai/greater-components-social": "workspace:*",
    "@equaltoai/greater-components-testing": "workspace:*",
    "@equaltoai/greater-components-tokens": "workspace:*",
    "@equaltoai/greater-components-utils": "workspace:*",
    "@vitest/coverage-v8": "^4.0.15",
    "vitest": "^4.0.15"
  }
}
