{
  "name": "@equaltoai/greater-components-cli",
  "private": true,
  "version": "0.1.2-rc.3",
  "description": "CLI for adding Greater Components to your project",
  "type": "module",
  "bin": {
    "greater": "./dist/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "tsc --watch --preserveWatchOutput",
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --exclude tests/e2e/smoke.test.ts",
    "test:coverage": "vitest run --coverage --exclude tests/e2e/smoke.test.ts",
    "test:e2e": "vitest run tests/e2e/smoke.test.ts"
  },
  "keywords": [
    "greater",
    "components",
    "cli",
    "activitypub",
    "fediverse",
    "svelte"
  ],
  "author": "Greater Components Team",
  "license": "AGPL-3.0-only",
  "repository": {
    "type": "git",
    "url": "https://github.com/equaltoai/greater-components.git",
    "directory": "packages/cli"
  },
  "dependencies": {
    "chalk": "^5.6.2",
    "commander": "^14.0.3",
    "execa": "^9.6.1",
    "fs-extra": "^11.3.3",
    "ora": "^9.3.0",
    "prompts": "^2.4.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^7.0.0",
    "@sveltejs/kit": "^2.50.2",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^25.2.2",
    "@types/prompts": "^2.4.9",
    "@vitest/coverage-v8": "^4.0.18",
    "hast-util-to-mdast": "^10.1.2",
    "mdast-util-gfm": "^3.1.0",
    "mdast-util-to-markdown": "^2.1.2",
    "rehype-parse": "^9.0.1",
    "rehype-sanitize": "^6.0.0",
    "rehype-stringify": "^10.0.1",
    "svelte-check": "^4.3.6",
    "typescript": "^5.9.3",
    "unified": "^11.0.5",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
