  "version": "1.0.0",
  "exports": {
    ".": "./src/index.ts",
    "./icons/*": "./src/icons/*.svelte"
  },
  "publish": {
    "include": [
      "src/**/*.ts",
      "src/**/*.svelte",
      "README.md",
      "CHANGELOG.md",
