{
  "name": "@equaltoai/greater-components-icons",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "description": "Icon components for Greater Components",
  "license": "AGPL-3.0-only",
  "author": "Greater Contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/equaltoai/greater-components.git",
    "directory": "packages/icons"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./icons/*.svelte": {
      "types": "./dist/icons/*.svelte.d.ts",
      "svelte": "./dist/icons/*.svelte",
      "import": "./dist/icons/*.svelte"
    },
    "./icons/*": {
      "types": "./dist/icons/*.svelte.d.ts",
      "svelte": "./dist/icons/*.svelte",
      "import": "./dist/icons/*.svelte"
    },
    "./icons/brands": {
      "types": "./dist/icons/brands/index.d.ts",
      "svelte": "./dist/icons/brands/index.js",
      "import": "./dist/icons/brands/index.js"
    },
    "./icons/brands/*.svelte": {
      "types": "./dist/icons/brands/*.svelte.d.ts",
      "svelte": "./dist/icons/brands/*.svelte",
      "import": "./dist/icons/brands/*.svelte"
    },
    "./icons/brands/*": {
      "types": "./dist/icons/brands/*.svelte.d.ts",
      "svelte": "./dist/icons/brands/*.svelte",
      "import": "./dist/icons/brands/*.svelte"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "node scripts/build.js && svelte-package --output dist",
    "dev": "node scripts/build.js --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "svelte": "^5.43.6"
  },
  "devDependencies": {
    "@sveltejs/package": "^2.5.6",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@types/node": "^24.10.1",
    "@vitest/coverage-v8": "^4.0.12",
    "chokidar": "^4.0.3",
    "feather-icons": "^4.29.2",
    "jsdom": "^27.2.0",
    "svelte": "^5.43.14",
    "typescript": "^5.9.3",
    "vite": "^7.2.4",
    "vitest": "^4.0.12"
  }
}
