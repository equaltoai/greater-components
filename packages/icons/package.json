{
  "name": "@equaltoai/greater-components-icons",
  "private": true,
  "version": "0.1.2-rc.4",
  "type": "module",
  "description": "Icon components for Greater Components",
  "license": "AGPL-3.0-only",
  "author": "Greater Contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/equaltoai/greater-components.git",
    "directory": "packages/icons"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./icons/*.svelte": {
      "types": "./dist/icons/*.svelte.d.ts",
      "svelte": "./dist/icons/*.svelte",
      "import": "./dist/icons/*.svelte"
    },
    "./icons/*": {
      "types": "./dist/icons/*.svelte.d.ts",
      "svelte": "./dist/icons/*.svelte",
      "import": "./dist/icons/*.svelte"
    },
    "./icons/brands": {
      "types": "./dist/icons/brands/index.d.ts",
      "svelte": "./dist/icons/brands/index.js",
      "import": "./dist/icons/brands/index.js"
    },
    "./icons/brands/*.svelte": {
      "types": "./dist/icons/brands/*.svelte.d.ts",
      "svelte": "./dist/icons/brands/*.svelte",
      "import": "./dist/icons/brands/*.svelte"
    },
    "./icons/brands/*": {
      "types": "./dist/icons/brands/*.svelte.d.ts",
      "svelte": "./dist/icons/brands/*.svelte",
      "import": "./dist/icons/brands/*.svelte"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "generate": "node scripts/build.js",
    "build": "svelte-package --output dist",
    "dev": "pnpm run generate -- --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "svelte": "^5.43.6"
  },
  "devDependencies": {
    "@sveltejs/package": "^2.5.7",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "@types/node": "^25.2.2",
    "@vitest/coverage-v8": "^4.0.18",
    "chokidar": "^5.0.0",
    "feather-icons": "^4.29.2",
    "jsdom": "^28.0.0",
    "svelte": "^5.50.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
